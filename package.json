{
	"name": "@nikosv/trustmebro",
	"version": "0.1.0",
	"description": "Fetch and index social media content for agentic workflows",
	"type": "module",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"cli",
		"ai",
		"social-media",
		"youtube",
		"reddit",
		"indexing",
		"search",
		"llm",
		"agent",
		"ai-agent"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/nichochar/trustmebro.git"
	},
	"bin": {
		"trustmebro": "dist/app.js"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": "./dist/app.js"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"scripts": {
		"dev": "bun run src/app.ts",
		"demo": "bun run src/demo-playground.ts",
		"build": "tsup src/app.ts --format esm --clean",
		"lint": "bunx --bun biome lint .",
		"fix": "bunx --bun biome format . --write",
		"check": "bunx --bun biome check .",
		"typecheck": "tsc --noEmit",
		"prepublishOnly": "npm run build",
		"changeset": "bunx --bun changeset",
		"version-packages": "bunx --bun changeset version",
		"release": "bunx --bun changeset publish"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.15",
		"@changesets/cli": "^2.29.8",
		"@types/node": "^22.0.0",
		"tsup": "^8.5.0",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@ai-sdk/amazon-bedrock": "3.0.57",
		"@ai-sdk/anthropic": "2.0.56",
		"@ai-sdk/azure": "2.0.82",
		"@ai-sdk/cerebras": "1.0.33",
		"@ai-sdk/cohere": "2.0.21",
		"@ai-sdk/deepinfra": "1.0.30",
		"@ai-sdk/gateway": "2.0.23",
		"@ai-sdk/google": "2.0.49",
		"@ai-sdk/google-vertex": "3.0.81",
		"@ai-sdk/groq": "2.0.33",
		"@ai-sdk/mistral": "2.0.26",
		"@ai-sdk/openai": "2.0.71",
		"@ai-sdk/openai-compatible": "^2.0.2",
		"@ai-sdk/perplexity": "2.0.22",
		"@ai-sdk/provider": "2.0.0",
		"@ai-sdk/provider-utils": "3.0.19",
		"@ai-sdk/togetherai": "1.0.30",
		"@ai-sdk/vercel": "1.0.31",
		"@ai-sdk/xai": "2.0.42",
		"@stricli/core": "^1.2.5",
		"ai": "^4.3.19",
		"greptor": "^0.8.0",
		"picocolors": "^1.1.1",
		"yaml": "^2.8.2",
		"zod": "^4.3.6"
	}
}
