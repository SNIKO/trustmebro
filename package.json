{
	"name": "trustmebro",
	"version": "0.0.0",
	"description": "TrustMeBro CLI app",
	"type": "module",
	"bin": {
		"trustmebro": "dist/app.js"
	},
	"files": [
		"dist"
	],
	"exports": {
		".": "./dist/app.js"
	},
	"private": true,
	"scripts": {
		"dev": "bun run src/app.ts",
		"build": "bun build src/app.ts --outdir dist --target bun --format esm && (printf '#!/usr/bin/env bun\n' | cat - dist/app.js > dist/.app.js) && mv dist/.app.js dist/app.js && chmod +x dist/app.js",
		"lint": "bunx --bun biome lint .",
		"fix": "bunx --bun biome format . --write",
		"check": "bunx --bun biome check .",
		"changeset": "bunx --bun changeset",
		"version-packages": "bunx --bun changeset version",
		"release": "bunx --bun changeset publish"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.11",
		"@changesets/cli": "^2.29.8",
		"@types/bun": "latest",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@ai-sdk/openai": "^1.3.0",
		"@ai-sdk/openai-compatible": "^2.0.4",
		"@stricli/core": "^1.2.0",
		"ai": "^4.3.0",
		"greptor": "../greptor",
		"picocolors": "^1.1.1",
		"pino": "^9.6.0",
		"pino-pretty": "^11.3.0",
		"yaml": "^2.8.1",
		"zod": "^4.3.5"
	}
}
